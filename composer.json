{
    "name": "backsystem/autocomplete",
    "type": "symfony-bundle",
    "description": "Autocomplete functionality for Symfony",
    "keywords": [
        "symfony",
        "autocomplete"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "symfony/dependency-injection": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/form": "^6.0",
        "doctrine/orm": "^2.9",
        "doctrine/doctrine-bundle": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "BackSystem\\Autocomplete\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "friendsofphp/php-cs-fixer": "^3.13"
    }
}
